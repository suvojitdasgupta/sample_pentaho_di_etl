    <connection>OBIAR</connection>
    <sql>SELECT
  bi.balance_impact_id AS balance_impact_id,
  'TRADEMORE' AS system_source,
  TO_CHAR(TO_DATE('${START_DT}', 'YYYY-MM-DD'),'YYYYMM') AS tax_period,
  '700260' AS market,
  NVL(LPAD(t.customer_taid,9,0),'000000000') AS geocode,
  ' ' AS city,
  ' ' AS state,
  ' ' AS county,
  'TM75038' AS store_id,
  bi.order_id AS  order_num,
  ' ' AS order_type,
  ' ' AS ind_bus,
  bill.bill_dt AS pay_date,
  --item_code
  --item_desc
  bi.quantity AS qty,
  td.taxable_amount/100 AS retail_net_price,
  td.amount/100 AS tax,
  0.00 AS discount,
  0.00 AS rebate,
  bi.product_id, -- Will be used to fetch prod_code1 and prod_code2
  ' ' AS loc_ful,
  ' ' AS dpc_group,
  ' ' AS dpc_item

FROM line_item_bill bill,
     billed_line_item_balance blib,
     line_item_balance bal,
     balance_impact bi,
     tax t,
     tax_detail td 
 
WHERE bill.line_item_bill_id = blib.line_item_bill_id
  AND blib.line_item_balance_id = bal.line_item_balance_id
  AND bal.line_item_balance_id = bi.line_item_balance_id
  AND bi.activity_reason_code = 'tax100' -- tax100 : Tax for the Original Charge
  AND bill.lineitem = 148
  AND bi.created_dt >= TO_DATE('${START_DT} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
  AND bi.created_dt &lt; TO_DATE('${END_DT} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
  AND bi.tax_id = t.tax_id
  AND t.tax_id = td.tax_id
  AND td.tax_holiday_ind='1'</sql>
    <limit>0</limit>
    <lookup />
    <execute_each_row>N</execute_each_row>
