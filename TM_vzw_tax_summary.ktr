    <name>Table input - Fetch Cash Credits and Tax</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>OBIAR</connection>
    <sql>SELECT 
       'C101' AS indicator,
       bill.line_item_bill_id, 
       bal.line_item_balance_id,
       bi.balance_impact_id,
       t.tax_id,  
       td.tax_type_id,
       TO_CHAR(cc.cash_credit_id) AS cash_credit_id,
       TO_CHAR(cc.issue_dt,'YYYY-MM-DD') AS activity_dt,
       'TRADEMORE' AS system_source,
       TO_CHAR(TO_DATE('${END_DT}', 'YYYY-MM-DD'),'YYYYMM') AS tax_period, -- For TradeMore it has to be END_DT
       '700260' AS market,
       (
        CASE WHEN t.state_province = 'AZ' THEN '490213'
             WHEN t.state_province = 'HI' THEN '490213'
             WHEN t.state_province = 'NM' THEN '490213'
             ELSE '224013' 
        END     
       ) AS account,
       'TM75038' AS store_id,
       NVL(LPAD(t.customer_taid,9,0),'000000000') AS vertex_geocode,
       NVL(tp.short_desc,'NONE') AS tp_short_desc,  -- will be used to derive tax_level
       '' AS juris_desc,
       (
        CASE WHEN t.state_province = 'AZ' THEN 'GR'
             WHEN t.state_province = 'HI' THEN 'GR'
             WHEN t.state_province = 'NM' THEN 'GR'
             ELSE 'SA' 
        END     
       ) AS tax_type,
       '' AS tax_category,
       CONCAT(CONCAT(t.state_province,'-'),td.imposition_type) AS tax_desc,
       td.taxable_amount/100 AS gross_revenue,
       'PHONE' AS revenue_desc,
       0.00 AS exempt_revenue,
       td.taxable_amount/100 AS taxable_revenue,
       td.rate AS tax_rate,
       td.amount/100 AS tax_amount,
       '' AS tax_auth_id

FROM cash_credit cc,
     line_item_bill bill,
     billed_line_item_balance blib,
     line_item_balance bal,
     balance_impact bi,
     tax t,
     tax_detail td,
     tax_type tp

WHERE 
      bi.created_dt >= TO_DATE('${START_DT} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
  AND bi.created_dt &lt; TO_DATE('${END_DT} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
  AND cc.lineitem=148
  AND cc.line_item_bill_id = bill.line_item_bill_id
  AND bill.line_item_bill_id = blib.line_item_bill_id
  AND blib.line_item_balance_id = bal.line_item_balance_id
  AND bal.line_item_balance_id = bi.line_item_balance_id
  AND bi.activity_reason_code = 'tax101' -- tax101 : All cash_credits except cash_credit_id=7
  AND bill.lineitem = 148
  AND bi.tax_id = t.tax_id
  AND t.tax_id = td.tax_id
  AND td.tax_type_id = tp.tax_type_id(+)

UNION ALL

SELECT 
       'C102' AS indicator,
       bill.line_item_bill_id, 
       bal.line_item_balance_id,
       bi.balance_impact_id,
       t.tax_id,  
       td.tax_type_id,
       TO_CHAR(cc.cash_credit_id) AS cash_credit_id,
       TO_CHAR(cc.issue_dt,'YYYY-MM-DD') AS activity_dt,
       'TRADEMORE' AS system_source,
       TO_CHAR(TO_DATE('${END_DT}', 'YYYY-MM-DD'),'YYYYMM') AS tax_period, -- For TradeMore it has to be END_DT
       '700260' AS market,
       (
        CASE WHEN t.state_province = 'AZ' THEN '490213'
             WHEN t.state_province = 'HI' THEN '490213'
             WHEN t.state_province = 'NM' THEN '490213'
             ELSE '224013' 
        END     
       ) AS account,
       'TM75038' AS store_id,
       NVL(LPAD(t.customer_taid,9,0),'000000000') AS vertex_geocode,
       NVL(tp.short_desc,'NONE') AS tp_short_desc,  -- will be used to derive tax_level
       '' AS juris_desc,
       (
        CASE WHEN t.state_province = 'AZ' THEN 'GR'
             WHEN t.state_province = 'HI' THEN 'GR'
             WHEN t.state_province = 'NM' THEN 'GR'
             ELSE 'SA' 
        END     
       ) AS tax_type,
       '' AS tax_category,
       CONCAT(CONCAT(t.state_province,'-'),td.imposition_type) AS tax_desc,
       td.taxable_amount/100 AS gross_revenue,
       'PHONE' AS revenue_desc,
       0.00 AS exempt_revenue,
       td.taxable_amount/100 AS taxable_revenue,
       td.rate AS tax_rate,
       td.amount/100 AS tax_amount,
       '' AS tax_auth_id

FROM cash_credit cc,
     line_item_bill bill,
     billed_line_item_balance blib,
     line_item_balance bal,
     balance_impact bi,
     tax t,
     tax_detail td,
     tax_type tp

WHERE 
      bi.created_dt >= TO_DATE('${START_DT} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
  AND bi.created_dt &lt; TO_DATE('${END_DT} 00:00:00', 'YYYY-MM-DD HH24:MI:SS')
  AND cc.lineitem=148
  AND cc.line_item_bill_id = bill.line_item_bill_id
  AND bill.line_item_bill_id = blib.line_item_bill_id
  AND blib.line_item_balance_id = bal.line_item_balance_id
  AND bal.line_item_balance_id = bi.line_item_balance_id
  AND bi.activity_reason_code = 'tax102' -- tax102 : For cash_credit_id=7
  AND bill.lineitem = 148
  AND bi.tax_id = t.tax_id
  AND t.tax_id = td.tax_id
  AND td.tax_type_id = tp.tax_type_id(+)
</sql>
