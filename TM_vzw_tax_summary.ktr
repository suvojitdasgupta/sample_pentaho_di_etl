  tmp_tax_level = 2
}
else if ((TP_SHORT_DESC == 'COUNTY') || (TP_SHORT_DESC == 'addl county tax')) {
   tmp_tax_level = 3
}
else if ((TP_SHORT_DESC == 'CITY') || (TP_SHORT_DESC == 'addl city tax')) {
   tmp_tax_level = 4
}
else if ((TP_SHORT_DESC == 'DISTRICT') || (TP_SHORT_DESC == 'addl district tax')) {
   tmp_tax_level = 5
}
else if (TP_SHORT_DESC == 'PARISH') {
   tmp_tax_level = 5
}
else {tmp_tax_level = 99}


</jsScript_script>
